@model IEnumerable<ICATask4.SkillService.SkillsDTO>

@{
    ViewBag.Title = "WcfResult";
}

<h2>WcfResult</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.skillCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.skillDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.skillCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.skillDescription)
        </td>
    </tr>
}

</table>
